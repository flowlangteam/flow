// Struct and implementation example
struct Point {
    pub x: i64,
    pub y: i64
}

func new_point(x: i64, y: i64) -> Point {
    Point { x: x, y: y }
}

impl Point {
    func distance_from_origin(point: Point) -> i64 {
        let x_sq = point.x * point.x;
        let y_sq = point.y * point.y;
        x_sq + y_sq
    }
}

func main() -> i64 {
    let p = new_point(3, 4);
    p.distance_from_origin()
}
