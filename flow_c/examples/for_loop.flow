// For loop example in Flow

func main() -> i64 {
    let mut sum = 0;
    let mut i = 0;
    
    // Simple for loop: sum numbers from 0 to 9
    for (i = 0; i < 10; i = i + 1) {
        sum = sum + i;
    };
    
    sum
}

func test_nested_for() -> i64 {
    let mut result = 0;
    let mut i = 0;
    let mut j = 0;
    
    // Nested for loops
    for (i = 0; i < 5; i = i + 1) {
        for (j = 0; j < 3; j = j + 1) {
            result = result + 1;
        }
    }
    
    result
}

func test_for_with_break_condition() -> i64 {
    let mut count = 0;
    let mut i = 0;
    
    // For loop with early termination logic
    for (i = 0; i < 100; i = i + 1) {
        count = count + 1;
        if count > 10 {
            i = 100; // Force loop to end
        }
    }
    
    count
}
